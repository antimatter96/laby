	</head>
	<body>
			<section id="banner">
				<header class="image">
					<img id="logo" src="static/logo.png">
					<h2>Labyrinthos</h2>
				</header>
			</section>	
		<article class="container box style3">
			<section>
					<header>
						<h3>Leaderboard</h3>
					</header>
					<div class="table-wrapper">
						<table class="default">
							<thead>
								<tr>
									<th>Rank</th>
									<th>Team Name</th>
									<th>Level</th>
								</tr>
							</thead>
							<tbody>
								{% for i in leaderboard %}
								<tr>
									<td>{{loop.index}}</td>
									<td>{{i.name}}</td>
									<td>{{i.level + 1}}</td>
								</tr>
								{% endfor %}
							</tbody>
						</table>
					</div>
				<section>
			</article>

			
		<section id="footer">
			<ul>
				<li><a href="http://facebook.com/events/251708241956711" class=""><span class="label">Facebook</span></a></li>
			</ul>
			<img id="moksha" src="static/mok.png">
			<div class="copyright image centered">
				<ul class="menu">
					<li>&copy; Labyrinthos. All rights reserved.</li><li>Design: <a href="http://html5up.net">HTML5 UP</a></li>
				</ul>
			</div>
		</section>
		
