<html>
	<head>
		{{ headers1.head("Leaderboard", "") }}
	</head>
	<body>
			<section id="banner">
				<header class="image">
					<img id="logo" src="static/logo.png">
					<h2>Labyrinthos</h2>
				</header>
			</section>	
		<article class="container box style3">
			<section>
					<header>
						<h3>Leaderboard</h3>
					</header>
					<div class="table-wrapper">
						<table class="default">
							<thead>
								<tr>
									<th>Rank</th>
									<th>Team Name</th>
									<th>Level</th>
								</tr>
							</thead>
							<tbody>
								{% for i in leaderboard %}
								<tr>
									<td>{{loop.index}}</td>
									<td>{{i.name}}</td>
									<td>{{i.level + 1}}</td>
								</tr>
								{% endfor %}
							</tbody>
						</table>
					</div>
				<section>
			</article>

		{% include "_footer.njk" %}
		{% include "_counter.njk" %}
	</body>
</html>
